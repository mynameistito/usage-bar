{
  "name": "usage-bar-windows",
  "version": "1.0.8",
  "description": "Windows system tray app for monitoring Claude, Z.ai, and Amp API usage",
  "type": "module",
  "scripts": {
    "cargo:check": "cd src-tauri && cargo check",
    "cargo:build": "cd src-tauri && cargo build",
    "cargo:build:release": "cd src-tauri && cargo build --release",
    "cargo:clippy": "cd src-tauri && cargo clippy",
    "cargo:test": "cd src-tauri && cargo test",
    "cargo:clean": "cd src-tauri && cargo clean",
    "cargo:fmt": "cd src-tauri && cargo fmt",
    "cargo:tree": "cd src-tauri && cargo tree",
    "cargo:precheck": "cd src-tauri && cargo check && cargo clippy -- -D warnings && cargo fmt --check",
    "tauri": "tauri",
    "dev": "tauri dev",
    "vite:dev": "vite",
    "vite:build": "vite build",
    "typecheck": "tsgo --noEmit",
    "build": "tauri build",
    "build:nsis": "tauri build --bundles nsis",
    "build:msi": "tauri build --bundles msi",
    "check": "ultracite check",
    "fix": "ultracite fix",
    "changeset": "changeset",
    "version": "changeset version && bun run sync-version",
    "sync-version": "bun scripts/sync-cargo-version.ts",
    "release": "bun scripts/create-release.ts",
    "prepare": "lefthook install",
    "regenerate-icon": "node scripts/svg-to-png.js"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.10.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.4.4",
    "@changesets/cli": "^2.29.8",
    "@tailwindcss/vite": "^4.2.1",
    "@tauri-apps/cli": "^2.10.0",
    "@typescript/native-preview": "^7.0.0-dev.20260225.1",
    "lefthook": "^2.1.1",
    "serve": "^14.2.5",
    "sharp": "^0.34.5",
    "tailwindcss": "^4.2.1",
    "ultracite": "7.2.3",
    "vite": "^7.3.1"
  },
  "overrides": {
    "ajv": "^8.18.0",
    "minimatch": "^10.2.1"
  },
  "author": "",
  "license": "ISC"
}
